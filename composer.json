{
    "name": "myhayo/walle-sdk",
    "description": "php sdk of walle-server",
    "type": "library",
    "homepage": "https://github.com/myhayo/walle-sdk",
    "require": {
        "php": "^7.1",
        "laravel/framework": "^5.6",
        "guzzlehttp/guzzle": "^7.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "wangyaoguang",
            "email": "wangyaoguang@myhayo.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Myhayo\\Walle\\": "src/"
        }
    },
    "extra": {
        "walle": {
            "providers": [
                "Myhayo\\Walle\\Providers\\WalleServiceProvider"
            ],
            "aliases": {
                "Walle": "Myhayo\\Walle\\Facades\\Walle"
            }
        }
    }
}
